<!-- First are the document type and the attributes for our document -->
<pc2xml version="$Revision: 8188 $" name="UT_SIP-UE 1.2.1.7" descrip="To Header and Tag Fields" number="UT_SIP-UE 1.2.1.7">
	<template name="UT_SIP-UE 1.2.1.7" file="./templates/ut/Term_DUT.xml"/>
	<remove_from fsm="UT_SIP-UE 1.2.1.7">
		<state ID="Setup" sub="response"/>
	</remove_from>
	<add_to fsm="UT_SIP-UE 1.2.1.7">
		<!-- SETUP State -->
		<state ID="Setup" sub="response">
			<!-- Expected responses are 100, 180, 183 or 200 -->
			<response>
				<!-- Step 4 from the Default Message Flow Call-Term-at-UE(DUT) Sequence Diagram -
               Test 18x response for reliability for possible Prack message -->
				<if>
					<eq>
						<msg_ref type="sip">Request</msg_ref>
						<expr>Invite</expr>
					</eq>
					<then>
						<send protocol="sip" msgtype="180-INVITE" target="UE1" originator="DUT"/>
					</then>
				</if>
			</response>
			<!-- Transitions for Setup state -->
			<transition to="Alerting" event="INVITE"/>
		</state>
	</add_to>
</pc2xml>
