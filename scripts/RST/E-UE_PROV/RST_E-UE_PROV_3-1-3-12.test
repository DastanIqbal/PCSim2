################################################################################
#// Description:
#//     TC2909 TEST CASE RST E-UE PROV 3.1.3.12 - Ability of eUE to Implement the
#//	    User Auto Callback Feature Table
#//
#//
#// Objective:
#//     This test verifies the eUE ability to properly implement the pktcEUERSTAutoCbTable
#//     as defined in CL-PKTC-EUE-RST-MIB.
#//     
#//
#// PacketCable DUT:
#//     EDVA
#//
#// Test Steps:
#//     - Initial configuration
#//     - Start sniffer
#//     - Reset DUT
#//     - Check Provisioning state
#//     - Invoke SNMPv3 API and query MIB objects and validate values
#//     - Stop sniffer
#//     - Hybrid configuration
#//     - Start sniffer
#//     - Reset DUT
#//     - Check Provisioning state
#//     - Invoke SNMPv2 API and bulk query MIB objects and validate values
#//     - Stop sniffer
#//
#//
#// Notes:
#//     - Requires provPrimitives.tcl
#//
#//
################################################################################

global SCRIPT_VERSION
set SCRIPT_VERSION { # $Id: RST_E-UE_PROV_3-1-3-12.test 7458 2010-08-06 15:34:50Z ghassler $ }


set test_case RST_E-UE_PROV_3-1-3-12-
set step_num 1
set NON_WRITABLE 2

Init_PROV

DoStep ${test_case}${step_num} "Initial Configuration" {
    LogMsg "--- Init PROV module ---"
    # PROV_MODIFY_DEVICE $MTA_MAC MTAFORCW_122 SecureFlow_BasicCall_DigitMap_ACB.cfg
    PROV_MODIFY_DEVICE $MTA_MAC MTAFORCW_122 C.1.1.24.bin
    PROV_DEPLOY_DHCP
} {PASS_STEP}

DoStep ${test_case}[incr step_num]_1 "Start sniffer" {
    LogMsg "--- Start sniffer ---"
    Init_CONTROLLER_LAN
    SetupCaptureControllerLan "host $MTA_IP || udp port 67"
    StartCaptureControllerLan
} {PASS_STEP}

DoStep ${test_case}${step_num}_2 "Reset CM/MTA" {
    set val [SNMPvN_RESET $CM_IP]
    LogMsg "--- Reset of CM and MTA has passed ---"
} {
    IsEqual $val 0
}

DoStep ${test_case}${step_num}_3 "pktcMTADevProvisioningState Content Verification" {
    # Wait for a minute to let CM/MTA starting to reboot
    actionWait "Wait for CM/MTA to reboot" 60
    # Get the OID from the MTA and check it using our previously defined
    set val [GetProvState 1 600]
    LogMsg "MTA Provisioning State"
    LogMsg "             - Expected : 1"
    LogMsg "             - Received : $val"
} {
    IsEqual $val 1
}


DoStep ${test_case}[incr step_num]_1 "pktcEUERSTACbTimer Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 1]
    LogMsg "The Value of pktcEUERSTACbTimer is: $val"
} {
    IsEqual $val 1800
}

DoStep ${test_case}${step_num}_2 "EUERSTACbSpRngDuration Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 2]
    LogMsg "The Value of EUERSTACbSpRngDuration is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}_3 "pktcEUERSTACbSpRngRetryTime Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 3]
    LogMsg "The Value of pktcEUERSTACbSpRngRetryTime is: $val"
} {
    IsEqual $val 15
}

DoStep ${test_case}${step_num}_4 "pktcEUERSTACbSpRngRetries Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 4]
    LogMsg "The Value of pktcEUERSTACbSpRngRetries is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}_5 "pktcEUERSTACbMaxSubSend Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 5]
    LogMsg "The Value of pktcEUERSTACbMaxSubSend is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}_6 "pktcEUERSTACbMaxSubRec Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 6]
    LogMsg "The Value of pktcEUERSTACbMaxSubRec is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}_7 "pktcEUERSTACbStatus Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMP_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 7]
    LogMsg "The Value of pktcEUERSTACbStatus is: $val"
} {
    IsEqual $val 1
}

DoStep ${test_case}[incr step_num] "Stop Sniffer" {
    LogMsg "--- Stop Sniffer ---"
    StopCaptureControllerLan
    SaveToFileControllerLan "_Secure"
} { PASS_STEP }

set step_num 1

DoStep ${test_case}${step_num}h "Initial Configuration" {
    LogMsg "--- Init PROV module ---"
    PROV_MODIFY_DEVICE $MTA_MAC HYBRID2
    PROV_DEPLOY_DHCP
} {PASS_STEP}

DoStep ${test_case}[incr step_num]h_1 "Start sniffer" {
    LogMsg "--- Start sniffer ---"
    Init_CONTROLLER_LAN
    SetupCaptureControllerLan "host $MTA_IP || udp port 67"
    StartCaptureControllerLan
} {PASS_STEP}

DoStep ${test_case}[incr step_num]h_2 "Reset CM/MTA" {
    set val [SNMPvN_RESET $CM_IP]
    LogMsg "--- Reset of CM and MTA has passed ---"
} {
    IsEqual $val 0
}

DoStep ${test_case}${step_num}h_3 "pktcMTADevProvisioningState Content Verification" {
    # Wait for a minute to let CM/MTA starting to reboot
    actionWait "Wait for CM/MTA to reboot" 120
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_GET $oid_pktcMtaDevProvisioningState $MTA_IP]
    LogMsg "MTA Provisioning State"
    LogMsg "             - Expected : 1"
    LogMsg "             - Received : $val"
} {
    IsEqual $val 1
}


DoStep ${test_case}[incr step_num]h_1 "pktcEUERSTACbTimer Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 1]
    LogMsg "The Value of pktcEUERSTACbTimer is: $val"
} {
    IsEqual $val 1800
}

DoStep ${test_case}${step_num}h_2 "EUERSTACbSpRngDuration Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 2]
    LogMsg "The Value of EUERSTACbSpRngDuration is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}h_3 "pktcEUERSTACbSpRngRetryTime Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 3]
    LogMsg "The Value of pktcEUERSTACbSpRngRetryTime is: $val"
} {
    IsEqual $val 15
}

DoStep ${test_case}${step_num}h_4 "pktcEUERSTACbSpRngRetries Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 4]
    LogMsg "The Value of pktcEUERSTACbSpRngRetries is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}h_5 "pktcEUERSTACbMaxSubSend Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 5]
    LogMsg "The Value of pktcEUERSTACbMaxSubSend is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}h_6 "pktcEUERSTACbMaxSubRec Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 6]
    LogMsg "The Value of pktcEUERSTACbMaxSubRec is: $val"
} {
    IsEqual $val 3
}

DoStep ${test_case}${step_num}h_7 "pktcEUERSTACbStatus Content Verification" {
    # Get the OID from the MTA and check it using our previously defined
    set val [SNMPvN_TABLE_GET $table_pktcEUERSTAutoCbTable.1 $MTA_IP 7]
    LogMsg "The Value of pktcEUERSTACbStatus is: $val"
} {
    IsEqual $val 1
}


DoStep ${test_case}[incr step_num]h "Stop Sniffer" {
    LogMsg "--- Stop Sniffer ---"
    StopCaptureControllerLan
    SaveToFileControllerLan "_Hybrid"
} { PASS_STEP }

DoStep ${test_case}[incr step_num] "Configure Provisioning Server" {
    PROV_MODIFY_DEVICE $MTA_MAC MTAFORCW_122 Device_Level_Secure.cfg
    PROV_DEPLOY_DHCP
} { PASS_STEP }


############### Reset CM and MTA and Verify Provisioning ################

DoStep ${test_case}[incr step_num] "Reset CM/MTA" {
    set val [SNMPvN_RESET $CM_IP]
    LogMsg "--- Reset of CM and MTA has passed ---"
} {
    IsEqual $val 0
}

DoStep ${test_case}[incr step_num] "pktcMTADevProvisioningState Content Verification" {
    # Wait for a minute to let CM/MTA starting to reboot
    actionWait "Wait for CM/MTA to reboot" 60
    # Get the OID from the MTA and check it using our previously defined
    set val [GetProvState 1 600]
    LogMsg "MTA Provisioning State"
    LogMsg "             - Expected : 1"
    LogMsg "             - Received : $val"
} {
    IsEqual $val 1
}


################################################################################
# Cleanup
################################################################################
set CONTINUE_PAST_STEP_ERROR 0
Cleanup


