<pc2xml version="$Revision: 8188 $" name="UT_SIP-UE 1.1.1.7" descrip="Not Applicable Header in RESPONSE are ignored" number="UT_SIP-UE 1.1.1.7">
	<template name="UT_SIP-UE 1.1.1.7" file="./templates//UT/Orig_DUT.xml"/>
	<add_to fsm="UT_SIP-UE 1.1.1.7">
		<states initialstate="PendingRegister1"/>
		<state ID="PendingRegister1" timer="500">
			<transition to="Register" event="TimerExpired"/>
		</state>
	</add_to>
	<remove_from fsm="UT_SIP-UE 1.1.1.7">
		<state ID="PendingRegister"/>
		<state ID="Register"/>
		<state ID="Digest"/>
		<state ID="Setup"/>
		<state ID="Proceeding"/>
		<state ID="Alerting"/>
		<state ID="Answer"/>
		<state ID="Connected"/>
	</remove_from>
	<add_to fsm="UT_SIP-UE 1.1.1.7">
		<state ID="Register">
			<prelude>
				<send protocol="sip" msgtype="Register" target="PCSCF1" originator="DUT"/>
			</prelude>
			<transition to="Digest" event="4xx-Register"/>
		</state>
		<state ID="Digest">
			<prelude>
				<sleep time="5000"/>
				<send protocol="sip" msgtype="Register" target="PCSCF1" originator="DUT"/>
			</prelude>
			<transition to="END" event="200-Register"/>
		</state>
	</add_to>
</pc2xml>
