<pc2xml version="$Revision: 8188 $" name="UT_SIP_Reg_Temp" descrip="This is a unit test script for testing the UE Registration template." number="1">
	<fsm name="UE0">
		<models>
			<register/>
		</models>
		<NE sim_type="term" elements="DUT" targets="UE0 DUT"/>
		<states initialstate="Register">
			<!-- REGISTER State -->
			<state ID="Register">
				<prelude>
					<send protocol="sip" msgtype="Register" target="PCSCF1" originator="DUT"/>
				</prelude>
				<transition to="Digest" event="401-Register"/>
				<transition to="Wait" event="200-Register"/>
			</state>
			<state ID="Digest">
				<prelude>
					<send protocol="sip" msgtype="Register" target="PCSCF1" originator="DUT"/>
				</prelude>
				<transition to="Wait" event="200-Register"/>
			</state>
			<!-- Wait State -->
			<state ID="Wait">
				<prelude>
					<sleep time="30000"/>
					<generate event="SucceedEvent"/>
				</prelude>
				<transition to="END" event="SucceedEvent"/>
			</state>
		</states>
	</fsm>
</pc2xml>
